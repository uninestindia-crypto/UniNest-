# Supabase credentials - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Razorpay credentials - Get these from your Razorpay dashboard
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Admin credentials - Set these to your desired values
ADMIN_EMAIL=your_admin_email@example.com
ADMIN_PASSWORD=your_secure_password

# Gemini API Key - Optional, only if using Google's Gemini AI
GEMINI_API_KEY=your_gemini_api_key

# Security: Admin setup secret for one-time initial admin creation
# Generate a secure random string for this value
ADMIN_SETUP_SECRET=your_secure_admin_setup_secret
