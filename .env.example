# Supabase credentials - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Razorpay credentials - Get these from your Razorpay dashboard
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Admin credentials - Set these to your desired values
ADMIN_EMAIL=your_admin_email@example.com
ADMIN_PASSWORD=your_secure_password

# Gemini API Key - Optional, only if using Google's Gemini AI
GEMINI_API_KEY=your_gemini_api_key

# Groq API Keys - Support rotation (indexed variables)
GROQ_API_KEY_1=your_key_1
GROQ_API_KEY_2=your_key_2
# ... up to GROQ_API_KEY_10

# Security: Admin setup secret for one-time initial admin creation
# Generate a secure random string for this value
ADMIN_SETUP_SECRET=your_secure_admin_setup_secret

# Sentry Error Tracking - Get these from sentry.io
# Create a free account and project at https://sentry.io
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Optional: For source map uploads (better error stack traces in Sentry)
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_ORG=your_sentry_org_name
SENTRY_PROJECT=your_sentry_project_name
